#############################################
# Frontend .env.example (Vite / Netlify)
# Copy to .env.local and fill with real values.
# Do NOT commit .env.local.
#############################################

# Backend base URL (Render)
VITE_BACKEND_URL=

# Supabase anon credentials (public)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Pi App ID (public identifier)
VITE_PI_APP_ID=

# Frontend public URL (Netlify)
VITE_FRONTEND_URL=

# Optional: feature flags
# VITE_ENABLE_PREMIUM=1
# VITE_LOG_LEVEL=debug
