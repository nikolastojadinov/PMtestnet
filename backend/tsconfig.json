{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "outDir": "dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false
  },
  "include": [
    "cron/**/*",
    "services/**/*",
    "utils/**/*",
    "regions.json"
  ]
}
