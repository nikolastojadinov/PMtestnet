# Timezone (Render koristi system TZ, ali je okej postaviti)
TZ=Europe/Budapest

# Supabase server-side
SUPABASE_URL=https://YOUR-SUPABASE-PROJECT.supabase.co
SUPABASE_SERVICE_ROLE=YOUR_SERVICE_ROLE_KEY

# YouTube API keys (exactly 3 expected; CSV)
YOUTUBE_API_KEYS=KEY1,KEY2,KEY3

# Cycle config
# CYCLE_START_DATE je dan kada je “Day 1” (format YYYY-MM-DD)
CYCLE_START_DATE=2025-10-21
# PHASE je opciono: 'fetch' | 'refresh' — auto-prelaz posle 29. dana važi uvek
PHASE=fetch
